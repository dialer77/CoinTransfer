<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon />
    <StartupObject>CoinTransfer.Program</StartupObject>
    <RootNamespace>CoinTransfer</RootNamespace>
    <AssemblyName>CoinTransfer</AssemblyName>
    <!-- WinForms는 Windows 전용. CA1416(플랫폼 호환성) 경고 비표시 -->
    <NoWarn>$(NoWarn);CA1416</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\ExchangeAPIController\ExchangeAPIController.csproj" />
  </ItemGroup>

</Project>
